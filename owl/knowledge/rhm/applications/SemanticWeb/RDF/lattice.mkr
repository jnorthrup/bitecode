do print od "","<--  original RDF ontology lattice -->" done;
do read from rdfVDLA.rdf done;
owl:Thing isc* ?;

do print od "","<--  simplify lattice -->" done;
do simplify lattice od owl:Thing done;

do print od "","<--  simplified RDF ontology lattice -->" done;
owl:Thing isc* ?;

! deletecomment od lattice.out to lattice.save done;
#exit;
