# attributes
Topic has
  r:id,		# <Topic r:id="rootname">
  catid,	# <catid>unique number</catid>
  d:Title,	# <d:Title>friendly label</d:Title>
  lastUpdate,	# <lastUpdate>yyyy-mm-dd hh:mm:ss</lastUpdate>
  narrow;	# <narrow r:resource="species rootname" />



# example
<Topic r:id="Top/UTF8">
  <catid>816296</catid>
  <d:Title>UTF8</d:Title>
</Topic>


