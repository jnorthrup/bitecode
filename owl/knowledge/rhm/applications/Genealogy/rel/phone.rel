r_phone is relation with
  arraymode=gdbm,arraykey="$1",
  label=[pid, phone, fax, email],
  format=[person:1, phone:2, phone:3, email:4],
  automatic=ISU,
  meaning={$1 has phone=$2,fax=$3,email=$4;};

